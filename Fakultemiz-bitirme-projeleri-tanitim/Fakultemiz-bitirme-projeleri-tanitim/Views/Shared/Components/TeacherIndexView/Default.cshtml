@{
    Layout = "~/Views/Shared/_PanelLayout.cshtml";
}
<!DOCTYPE html>
<html>
<body>
    <div id="wrapper">
        <nav class="navbar-default navbar-static-side" role="navigation">
            <div class="sidebar-collapse">
                <ul class="nav metismenu" id="side-menu">
                    <li class="nav-header">
                        <div class="dropdown profile-element">
                            <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                                <span class="block m-t-xs font-bold">Bilge Gökkaya</span>
                                <span class="text-muted text-xs block">Akademik Danışman</span>
                            </a>
                        </div>
                    </li>
                    <li class="active">
                        <a href="@Url.Action("Index","Teacher")">
                            <i class="fa fa-th-large"></i>
                            <span class="nav-label">Panel</span>
                            <span class="fa arrow"></span>
                        </a>
                        <ul class="nav nav-second-level">
                            <li class="active">
                                <a href="@Url.Action("Index","Teacher")">Proje Yayınlama </a>
                            </li>
                            <li><a href="project_view.html">Proje Görüntüleme</a></li>


                        </ul>
                    </li>
                </ul>
            </div>
        </nav>

        <div id="page-wrapper" class="gray-bg dashbard-1">
            <div class="row border-bottom">
                <nav class="navbar navbar-static-top"
                     role="navigation"
                     style="margin-bottom: 0">
                    <div class="navbar-header">
                        <a class="navbar-minimalize minimalize-styl-2 btn btn-primary"
                           href="#">
                            <i class="fa fa-bars"></i>
                        </a>
                    </div>
                    <ul class="nav navbar-top-links navbar-right">
                        <li style="padding: 20px">
                            <span class="m-r-sm text-muted welcome-message">ISUBÜ TEKNOLOJİ FAKÜLTESİ BİTİRME PROJELERİ AKADEMİK DANIŞMAN SAYFASI </span>
                        </li>

                        <li>
                            <a href="login.html">
                                <i class="fa fa-sign-out"></i> Log out
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>

            <div class="row">
                <div class="col-lg-12">
                    <div class="ibox ">
                        <div class="ibox-title">
                            <h5>Proje Yayınlama Formu</h5>

                        </div>
                        <div class="ibox-content">

                            <div class="table-responsive">
                                <table class="table table-striped table-bordered table-hover dataTables-example">
                                    <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>Proje Adı</th>
                                            <th>Proje Sahibinin Adı</th>
                                            <th>Bölümü</th>
                                            <th>Yılı</th>
                                            <th>Konusu</th>
                                            <th>özeti</th>
                                            <th>Resim 1</th>
                                            <th>Resim 2</th>
                                            <th>Resim 3</th>
                                            <th>Resim 4</th>
                                            <th>Yayınlansın mı</th>
                                        </tr>

                                    </thead>

                                    <tbody>
                                        <tr class="gradeX">
                                            <td>Trident</td>
                                            <td>
                                                Internet
                                                Explorer 4.0
                                            </td>
                                            <td>Win 95+</td>
                                            <td class="center">4</td>
                                            <td class="center">X</td>
                                            <td>Trident</td>
                                            <td>
                                                Internet
                                                Explorer 4.0
                                            </td>
                                            <td>Win 95+</td>
                                            <td class="center">4</td>
                                            <td class="center">X</td>
                                            <td class="center">X</td>
                                            <td><input type="checkbox" class="i-checks" name="input[]"></td>
                                        </tr>
                                        <tr class="gradeC">
                                            <td>Trident</td>
                                            <td>
                                                Internet
                                                Explorer 5.0
                                            </td>
                                            <td>Win 95+</td>
                                            <td class="center">5</td>
                                            <td class="center">C</td>
                                            <td>Trident</td>
                                            <td>
                                                Internet
                                                Explorer 4.0
                                            </td>
                                            <td>Win 95+</td>
                                            <td class="center">4</td>
                                            <td class="center">X</td>
                                            <td class="center">X</td>
                                            <td><input type="checkbox" class="i-checks " name="input[]"></td>
                                        </tr>


                                    </tbody>

                                </table>

                            </div>
                            <div class="form-group row btn-kaydet">
                                <button class="btn btn-lg btn-primary float-right m-t-n-xs"
                                        type="submit">
                                    <strong>Kaydet</strong>
                                </button>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>

</body>
</html>
