@using EntityLayer.Concrete
@model List<Project>
@{
    Layout = "~/Views/Shared/_PanelLayout.cshtml";
}
<!DOCTYPE html>
<html>
<body>
    <div id="wrapper">
        <nav class="navbar-default navbar-static-side" role="navigation">
            <div class="sidebar-collapse">
                <ul class="nav metismenu" id="side-menu">
                    <li class="nav-header">
                        <div class="dropdown profile-element">
                            <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                                <span class="block m-t-xs font-bold">Akademik Danışman Paneli</span>
                                <span class="text-muted text-xs block">Kullanıcı Adı:@ViewBag.UserName</span>
                            </a>
                        </div>
                    </li>
                    <li class="active">
                        <a href="@Url.Action("Index","Teacher")">
                            <i class="fa fa-th-large"></i>
                            <span class="nav-label">Panel</span>
                            <span class="fa arrow"></span>
                        </a>
                        <ul class="nav nav-second-level">
                            <li>
                                <a href="@Url.Action("Index","Teacher")">Proje Yayınlama </a>
                            </li>
                            <li><a href="@Url.Action("ProjectStatus","Teacher")">Proje Görüntüleme</a></li>
                            <li class="active"><a href="@Url.Action("ProjectDelete","Teacher")">Proje Silme</a></li>
                            <li><a href="@Url.Action("StudentInformation","Teacher")">Öğrenci Bilgileri</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </nav>

        <div id="page-wrapper" class="gray-bg dashbard-1">
            @await Html.PartialAsync("~/Views/Shared/Partial/PanelNavbarPartial.cshtml")

            <div class="row">
                <div class="col-lg-12">
                    <div class="ibox ">
                        <div class="ibox-title">
                            <h5>Proje Silme Formu</h5>

                        </div>
                        <div class="ibox-content">

                            <div class="table-responsive">
                                <table class="table table-striped table-bordered table-hover dataTables-example">
                                    <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>Proje Adı</th>
                                            <th>Proje Sahibinin Adı</th>
                                            <th>Bölümü</th>
                                            <th>Yılı</th>
                                            <th>Konusu</th>
                                            <th>özeti</th>
                                            <th>Resim 1</th>
                                            <th>Resim 2</th>
                                            <th>Resim 3</th>
                                            <th>Resim 4</th>
                                            <th>Silinsin mi</th>
                                        </tr>

                                    </thead>

                                    <tbody>
                                        <tr class="gradeX">
                                            <td>Trident</td>
                                            <td>
                                                Internet
                                                Explorer 4.0
                                            </td>
                                            <td>Win 95+</td>
                                            <td class="center">4</td>
                                            <td class="center">X</td>
                                            <td>Trident</td>
                                            <td>
                                                Internet
                                                Explorer 4.0
                                            </td>
                                            <td>Win 95+</td>
                                            <td class="center">4</td>
                                            <td class="center">X</td>
                                            <td class="center">X</td>
                                            <td>
                                                <button class="btn btn-lg btn-primary  m-t-n-xs"
                                                        type="submit">
                                                    <strong>Sil</strong>
                                                </button>
                                            </td>
                                        </tr>
                                        <tr class="gradeC">
                                            <td>Trident</td>
                                            <td>
                                                Internet
                                                Explorer 5.0
                                            </td>
                                            <td>Win 95+</td>
                                            <td class="center">5</td>
                                            <td class="center">C</td>
                                            <td>Trident</td>
                                            <td>
                                                Internet
                                                Explorer 4.0
                                            </td>
                                            <td>Win 95+</td>
                                            <td class="center">4</td>
                                            <td class="center">X</td>
                                            <td class="center">X</td>
                                            <td>
                                                <button class="btn btn-lg btn-primary  m-t-n-xs"
                                                        type="submit">
                                                    <strong>Sil</strong>
                                                </button>
                                            </td>
                                        </tr>


                                    </tbody>

                                </table>

                            </div>

                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
